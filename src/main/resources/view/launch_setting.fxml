<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXRadioButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import de.jensd.fx.glyphs.materialdesignicons.MaterialDesignIconView?>
<VBox styleClass="background" alignment="CENTER" prefHeight="350.0" prefWidth="400.0" spacing="20.0" stylesheets="/css/main.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pa.am.video_catcher.controller.LaunchSettingController">
    <JFXRadioButton fx:id="rBtn_launchSimple" text="以极简界面启动" />
    <JFXRadioButton fx:id="rBtn_launchPro" text="以高级界面启动" />
    <HBox alignment="CENTER" prefHeight="60.0" spacing="60.0">
        <JFXButton prefHeight="55.0" prefWidth="110.0" text="确 定" styleClass="btn-primary" onMouseClicked="#click_commit">
            <graphic><MaterialDesignIconView glyphName="COMMENT_CHECK" glyphSize="24" styleClass="icon"/></graphic>
        </JFXButton>
        <JFXButton prefHeight="55.0" prefWidth="110.0" text="返 回" styleClass="btn-secondary" onMouseClicked="#click_return">
            <graphic><MaterialDesignIconView glyphName="KEYBOARD_RETURN" glyphSize="24" styleClass="icon"/></graphic>
        </JFXButton>
        <VBox.margin><Insets top="45.0" /></VBox.margin>
    </HBox>
</VBox>
